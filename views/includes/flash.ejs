
<%if(success && success.length) { %>
   <br> <div class="alert alert-success alert-dismissible fade show col-6 offset-3 mt-5" role="alert">
        <%= success %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="close">
        </button>
    </div>

<% } %>

 
<% if(error && error.length) { %>
    <br><div class="alert alert-danger alert-dismissible fade show col-6 offset-3 mt-5" role="alert">
        <%= error %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="close">
        </button>
    </div>

<% } %>
