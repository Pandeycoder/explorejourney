
<% layout("/layouts/bolierplate") %>





 



<div class="container">
<div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1 listing-card-style ">
    <% for( listing of allListings) { %>
        <a href="/<%= listing._id %>" class="listing-list">


            <div class="card col listing-card  position-relative">
                <div class="share-icon">
                    <i class="fa-solid fa-share  position-absolute end-0 top-0 m-1"></i>
                </div>
                   <i class="fa-regular fa-heart position-absolute end-0 top-0 m-3" ></i>


                <div class="carousel slide mt-3"  id="banner_<%= listing._id %>">
                    <div class="carousel-indicators">
                      <div class="carousel-indicators">
                          <% for (let i = 0; i < listing.image.length; i++) { %>
                              <button data-bs-slide-to="<%= i %>" class="<%= i === 0 ? 'active' : '' %> mb-3 me-3" style="border: none; width: 0px; height: 0px;">
                                  <span class="circle-indicator"></span>
                              </button>
                          <% } %>
                      </div>
                        <!-- <button data-bs-slide-to="0" class="active mb-3 me-3" style="border: none; width: 0px; height:0px;">
                            <span class="circle-indicator"></span>
                        </button>
                        <button data-bs-slide-to="1" class=" me-3" style="border: none; width: 0px; height:0px;">
                            <span class="circle-indicator"></span>
                        </button>
                        <button data-bs-slide-to="2" class="me-3" style="border: none; width: 0px; height:0px;">
                            <span class="circle-indicator"></span>
                        </button>
                        <button data-bs-slide-to="3" class=" me-3" style="border: none; width: 0px; height:0px;">
                          <span class="circle-indicator"></span>
                      </button>
                      <button data-bs-slide-to="4" class=" me-3" style="border: none; width: 0px; height:0px;">
                          <span class="circle-indicator"></span>
                      </button> -->
                       
                       
                    </div>
                    <div class="carousel-inner">
                      <% listing.image.forEach((image, index) => { %>
                         
                         
                          <div class="carousel-item <%= index === 0 ? 'active' : '' %>" data-bs-interval="false">
                              <img src="<%= image.url %>" class="card-img-top" alt="<%= image.filename %>"  style="height: 20rem;" loading="lazy">
                              <div class="card-img-overlay"></div>
                            </div>
                      
                          <% }); %>
                      
                        <button class="carousel-control-prev" data-bs-slide="prev" data-bs-target="#banner_<%= listing._id %>">
                            <span class="carousel-control-prev-icon btn btn-danger" style="border-radius: 50%;"></span>
                        </button>
                        <button class="carousel-control-next" data-bs-slide="next" data-bs-target="#banner_<%= listing._id %>">
                            <span class="carousel-control-next-icon btn btn-success" style="border-radius: 50%; font-size: 5px !important;"></span>
                        </button>
                    </div>
                </div>
                <div class="card-body">
                    <p class="card-text">
                        <b>
                            <%= listing.title %>
                        </b>
                        <br>
                        
                            <i class="taxt-info">&nbsp;&nbsp; +18% GST</i>
                    </p>
                </div>
            </div>
        </a>
        <% } %>
</div>
</div>


